<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.MainActivity">

    <com.padc.ponnya.thelibraryapp.views.components.CircleImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:firstArcColor="@color/blue"
        app:secondArcColor="@color/green"
        app:thirdArcColor="@color/yellow"
        app:fourthArcColor="@color/dark_orange"
        app:innerCircleColor="@color/white"
        android:src="@drawable/profile"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>